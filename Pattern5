public class Main {
    public static void main(String[] args) {
       pattern5(5);
    }

    private static void pattern5(int n) {
        for(int row = 1; row <= n * 2; row++){
            int stars = row <= n ? row : n * 2 - row;
            for(int col = 1; col <= stars; col++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
